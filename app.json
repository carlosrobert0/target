{
  "expo": {
    "name": "cofrin",
    "slug": "cofrin",
    "scheme": "cofrin",
    "version": "1.0.0",
    "userInterfaceStyle": "automatic",
    "android": {
      "package": "com.carlos.cofrin",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3D44CD"
      }
    },
    "ios": {
      "bundleIdentifier": "com.carlos.cofrin",
      "icon": "./assets/ios-icon.png"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-sqlite",
      [
        "expo-splash-screen",
        {
          "android": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#f1f5f5"
          },
          "ios": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#1e293b"
          },
          "imageWidth": 500
        }
      ],
      "react-native-edge-to-edge"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "77ade8e7-b799-481e-acc0-ee56b2942a20"
      }
    }
  }
}